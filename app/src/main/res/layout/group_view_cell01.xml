<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="@dimen/group_cell_height"
    android:background="@color/bg_white">

<jp.co.webshark.on2.customViews.HttpImageView
    android:id="@+id/member_image"
    android:layout_width="@dimen/group_cell_height"
    android:layout_height="@dimen/group_cell_height"
    android:layout_marginTop="@dimen/activity_vertical_margin_quarter"
    android:layout_marginLeft="@dimen/activity_horizontal_margin_half"
    android:layout_marginBottom="@dimen/activity_vertical_margin_quarter"
    android:src="@drawable/list_icon_user"/>

<TextView
    android:id="@+id/cell_member_name"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="member name"
    android:layout_toRightOf="@+id/member_image"
    android:layout_marginLeft="@dimen/activity_vertical_margin"
    android:layout_toLeftOf="@+id/cell_switch_button"
    android:layout_marginRight="@dimen/activity_vertical_margin"
    android:layout_centerVertical="true"/>

<ImageView
    android:id="@+id/cell_switch_button"
    android:onClick="memberOnOff"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:layout_marginRight="@dimen/activity_vertical_margin"
    android:src="@drawable/setup_check"
    android:layout_centerVertical="true"/>

<TextView
    android:layout_width="1px"
    android:layout_height="1px"
    android:text=""
    android:id="@+id/cellHiddenIndex"/>
</RelativeLayout>
