<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/invite_friends_cell_height"
    android:background="@color/bg_white">

    <TextView
        android:layout_width="1px"
        android:layout_height="1px"
        android:text=""
        android:id="@+id/cellHiddenIndex"/>

    <TextView
        android:id="@+id/cellName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="name"
        android:layout_marginLeft="@dimen/activity_vertical_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>

    <TextView
        android:id="@+id/cellTel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tel"
        android:layout_below="@+id/cellName"
        android:layout_marginLeft="@dimen/activity_vertical_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>

    <jp.co.webshark.on2.customViews.EffectImageView
        android:id="@+id/switch_icon"
        android:onClick="sendSMS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="@dimen/activity_vertical_margin"
        android:src="@drawable/setup_invite"
        android:layout_centerVertical="true"/>
</RelativeLayout>
